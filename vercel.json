{
    "builds": [
      {
        "src": "public/*",
        "use": "@vercel/static"
      },
      {
        "src": "api/index.php",
        "use": "@vercel/php"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "public/index.php"
      }
    ],
    "rewrites": [
    {
        "source": "/(.*)",
        "destination": "/public/index.php"
    }
    ]
  }
  